 retrieve=" SELECT 'Authorization to Disclose' as groups, b.event_type_desc,   
         c.event_specific_desc,   
         a.event_no, a.event_comment,cl.claim_no as 'id_num', a.create_date
    from CLAIM cl, CLAIM_EVENT a, Event_Type b, Event_Specific c
  where  cl.claim_no = a.claim_no
  and    a.event_type_code = b.event_type_code
and      a.event_type_code = c.event_type_code
and      a.event_specific_code = c.event_specific_code
 and     a.event_type_code = '060'
  and    cl.individual_no = :al_individual_no
  union all
  
  select 'Caution' as groups, IsNull(d.event_type_desc,'Caution'), e.event_specific_desc, c.individual_event_no, c.event_comment, 0 as 'id_num', c.create_date
  from  INDIVIDUAL b
   left join INDIVIDUAL_EVENT c  on  b.individual_no = c.individual_no
   join       Event_Type d on c.event_type_code = d.event_type_code
   join      Event_Specific e on c.event_type_code = e.event_type_code
and      c.event_specific_code = e.event_specific_code
  where   b.caution_flag = 'Y'
and     c.individual_event_no = (select max(f.individual_event_no) 
                               from  INDIVIDUAL_EVENT f
                               where c.individual_no = f.individual_no
                                  and  (f.event_type_code = '034' and f.event_specific_code <> 'C5'))
  and    b.individual_no = :al_individual_no
  union all
  select  'Drug Alert' as groups, c.drug_alert_type_desc, CASE WHEN b.terminated_date IS NULL THEN 'N' ELSE 'Y' END, b.drug_alert_no, b.alert_comment, 0 as 'id_num', 
           IsNull(b.terminated_date, b.create_date)
  from DRUG_ALERT b, Drug_Alert_Type c
  WHERE  b.drug_alert_type_code = c.drug_alert_type_code
  AND   b.effective_date <= getdate()
  AND   b.individual_no = :al_individual_no
union all
select 'Special Authorizations Expiring' as groups, a.din_gp_pin_desc,'', 0, '', b.claim_no as 'id_num', a.special_authorization_end_date 
from RX_SPECIAL_AUTHORIZATION a, CLAIM b
WHERE a.claim_no = b.claim_no
and  (DateDiff(d, getdate(),a.special_authorization_end_date) <= 30 AND DateDiff(d, getdate(),a.special_authorization_end_date) > 0)
and  b.individual_no = :al_individual_no

ORDER BY groups, event_no desc" arguments=(("al_individual_no", number)) )
