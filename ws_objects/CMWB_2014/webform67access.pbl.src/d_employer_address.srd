 column=(type=char(10) update=yes updatewhereclause=yes name=employer_address_postal_code dbname="postal_code" )
 column=(type=char(25) update=yes updatewhereclause=yes name=employer_address_city dbname="city" )
 column=(type=char(2) update=yes updatewhereclause=yes name=employer_address_prov_state_code dbname="prov_state_code" initial="NB" )
 column=(type=char(3) update=yes updatewhereclause=yes name=employer_address_country_code dbname="country_code" initial="CAN" )
 column=(type=char(60) updatewhereclause=yes name=employer_name dbname="employer_name" )
 column=(type=long updatewhereclause=yes name=web_access dbname="web_access" )
 retrieve="
  select   a.snb_form67_efile_flag, b.employer_no ,    b.address_line1 ,          
           b.address_line2 ,        b.address_line3 ,  b.postal_code ,           
		   b.city ,                 b.prov_state_code, b.country_code ,
		   d.employer_name, isnull(e.employer_no,0) as 'web_access'
  from EMPLOYER				a
  join EMPLOYER_ADDRESS		b on a.employer_no			= b.employer_no
  join Address_Type			c on b.address_type_code	= c.address_type_code
  join EMPLOYER_NAME		d on d.employer_no			= a.employer_no and d.employer_name_type_code = 'L'
  left join WEB_EMPLOYER_FORM67_ACCESS e on a.employer_no = e.employer_no
  where b.address_type_code	= 'BA'
  and a.employer_no			= :employer_no" arguments=(("employer_no", number)) )
