 column=(type=char(1) updatewhereclause=yes name=employer_type_code dbname="employer_type_code" )
 column=(type=char(1) updatewhereclause=yes name=operation_status_code dbname="operation_status_code" )
 column=(type=datetime updatewhereclause=yes name=operation_start_date dbname="operation_start_date" )
 column=(type=datetime updatewhereclause=yes name=operation_inactive_date dbname="operation_inactive_date" )
 column=(type=char(1) updatewhereclause=yes name=volunteer_flag dbname="volunteer_flag" )
 column=(type=char(1) updatewhereclause=yes name=billing_cycle_code dbname="billing_cycle_code" )
 column=(type=char(6) updatewhereclause=yes name=classification_system_code dbname="classification_system_code" )
 column=(type=char(2) updatewhereclause=yes name=classification_system_type_code dbname="classification_system_type_code" )
 retrieve="SELECT a.employer_no,
            a.operation_no,
            a.sold_to_employer_no,
            a.sold_to_operation_no,
            b.employer_type_code,
            a.operation_status_code,
            a.operation_start_date,
            a.operation_inactive_date,
            a.volunteer_flag,
            b.billing_cycle_code, 
            OC.classification_system_code,
            OC.classification_system_type_code

FROM            OPERATION                a
INNER JOIN      EMPLOYER                 b  ON a.employer_no = b.employer_no
LEFT OUTER JOIN OPERATION_CLASSIFICATION OC ON a.employer_no = OC.employer_no 
                                           AND a.operation_no = OC.operation_no
                                           AND OC.active_flag = 'Y'
WHERE    a.employer_no = :al_employer_no and a.operation_no = :al_operation_no" arguments=(("al_employer_no", number),("al_operation_no", number)) )
