 retrieve="SELECT  distinct  (a.authorization_group_no), 
             sum(a.total_payment_amount),      
        a.authorization_group_date,
       a.authorization_group_user_id,
        a.authorization_group_reason_code,
        a.authorization_group_reason_comment,
        b.comment_required, 
        c.authorization_type_code,
        a.authorized_by_code,
        a.authorized_date
FROM    PAYMENT   a
JOIN    Authorization_Group_Reason b ON a.authorization_group_reason_code = b.authorization_group_reason_code
JOIN    Payment_Combination c ON a.payment_type_code = c.payment_type_code
LEFT JOIN OPENING d ON a.opening_no = d.opening_no AND a.claim_no = d.claim_no AND c.opening_type_code = d.opening_type_code
WHERE   a.authorization_group_no > 0
AND    a.processed_date IS NULL
AND    a.claim_no = :al_claim_no
group by a.authorization_group_no, a.authorization_group_date,
       a.authorization_group_user_id,
        a.authorization_group_reason_code,
        a.authorization_group_reason_comment,
        b.comment_required,
        c.authorization_type_code,
 a.authorized_by_code,
        a.authorized_date" arguments=(("al_claim_no", number)) )
