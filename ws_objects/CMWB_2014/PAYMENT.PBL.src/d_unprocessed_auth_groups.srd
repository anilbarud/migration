 column=(type=char(2) updatewhereclause=yes name=authorization_group_reason_code dbname="authorization_group_reason_code" )
 column=(type=char(80) updatewhereclause=yes name=authorization_group_reason_comment dbname="authorization_group_reason_comment" )
 column=(type=char(1) updatewhereclause=yes name=comment_required dbname="comment_required" )
 column=(type=char(4) updatewhereclause=yes name=authorization_type_code dbname="authorization_type_code" )
 column=(type=char(16) updatewhereclause=yes name=authorized_by_code dbname="authorized_by_code" )
