table(column=(type=decimal(2) updatewhereclause=yes name=net_annuity_payout_amount dbname="net_annuity_payout_amount" )
 column=(type=char(53) updatewhereclause=yes name=name_on_cheque dbname="name_on_cheque" )
 column=(type=char(1) updatewhereclause=yes name=annuity_payout_recipient_type_code dbname="annuity_payout_recipient_type_code" )
 column=(type=char(20) updatewhereclause=yes name=recipient_type_desc dbname="recipient_type_desc" )
 column=(type=char(16) updatewhereclause=yes name=authorized_by_user_id dbname="authorized_by_user_id" )
 column=(type=datetime updatewhereclause=yes name=authorized_date dbname="authorized_date" )
 retrieve="SELECT a.net_annuity_payout_amount,
       a.name_on_cheque,
       a.annuity_payout_recipient_type_code,
       b.recipient_type_desc,
        c.authorized_by_user_id,
        c.authorized_date
FROM   ANNUITY_PAYOUT_RECIPIENT a 
JOIN   Recipient_Type b ON a.annuity_payout_recipient_type_code = b.recipient_type_code
JOIN   ANNUITY_PAYOUT c ON a.annuity_payout_no = c.annuity_payout_no AND a.annuity_account_no =c.annuity_account_no
WHERE  a.annuity_payout_no = :al_annuity_payout_no

UNION ALL

SELECT  sum(a.payment_amount * -1),
        name_on_cheque = (Select given_names + ' ' + last_name + ' (Write-off)'
                          FROM INDIVIDUAL
                          Where individual_no = a.recipient_no), 
                               
        a.recipient_type_code,
        b.recipient_type_desc,
        c.authorized_by_user_id,
        c.authorized_date
FROM    ANNUITY_PAYOUT_TXN_DETAIL a 
JOIN    Recipient_Type b ON a.recipient_type_code = b.recipient_type_code
JOIN   ANNUITY_PAYOUT c ON a.annuity_payout_no = c.annuity_payout_no AND a.annuity_account_no = c.annuity_account_no
WHERE   a.annuity_payout_no = :al_annuity_payout_no
AND     a.payment_sub_type_code = 'CW'
group by a.recipient_no,a.recipient_type_code,b.recipient_type_desc,  c.authorized_by_user_id,        c.authorized_date
order by b.recipient_type_desc" arguments=(("al_annuity_payout_no", number)) )
